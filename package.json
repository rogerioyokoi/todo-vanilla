{
  "name": "todo-vanilla",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:open": "nyc npm run test:coverage && npx http-server -c-1 coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:format": "prettier --ignore-unknown",
    "lint:format:fix": "prettier --ignore-unknown --write"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.2.0",
    "globals": "^15.3.0",
    "http-server": "^14.1.1",
    "jsdom": "^24.1.0",
    "nyc": "^15.1.0",
    "prettier": "3.2.5",
    "vite": "^5.2.0",
    "vitest": "^1.6.0",
    "vitest-axe": "^0.1.0"
  },
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3"
  }
}
